SELECT *
WHERE {
  ?v0 <http://www.w3.org/2000/01/rdf-schema#label> ?v1 .
  ?v0 <http://dbpedia.org/property/abstract> ?v2 .
  ?v0 <http://dbpedia.org/property/city> ?v3 .
  ?v3 <http://www.w3.org/2000/01/rdf-schema#label> ?v4 .
  ?v0 <http://dbpedia.org/property/country> ?v5 .
  ?v5 <http://www.w3.org/2000/01/rdf-schema#label> ?v6 .
}
###
SELECT *
WHERE {
  ?v0 <http://www.w3.org/2004/02/skos/core#broader> ?v1 .
  ?v2 <http://www.w3.org/2004/02/skos/core#broader> ?v0 .
  ?v3 <http://www.w3.org/2004/02/skos/core#subject> ?v2 .
  ?v3 <http://dbpedia.org/property/fuelType> ?v4 .
  ?v3 <http://dbpedia.org/property/maxCapacity> ?v5 .
  ?v3 <http://xmlns.com/foaf/0.1/page> ?v6 .
}
